{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel",
        "livewire",
        "foxws"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "artesaos/seotools": "^1.3.1",
        "blade-ui-kit/blade-heroicons": "^2.3.0",
        "calebporzio/sushi": "^2.5.2",
        "foxws/wireuse": "^1.0.5",
        "guzzlehttp/guzzle": "^7.8.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^11.11.1",
        "laravel/horizon": "^5.24.5",
        "laravel/reverb": "@beta",
        "laravel/sanctum": "^4.0.2",
        "laravel/scout": "^10.10.0",
        "laravel/tinker": "^2.9.0",
        "livewire/livewire": "^3.5.1",
        "meilisearch/meilisearch-php": "^1.8.0",
        "pusher/pusher-php-server": "^7.2.4",
        "spatie/commonmark-wire-navigate": "^1.0",
        "spatie/laravel-activitylog": "^4.8.0",
        "spatie/laravel-collection-macros": "^7.14.0",
        "spatie/laravel-db-snapshots": "^2.6.1",
        "spatie/laravel-google-fonts": "^1.4.1",
        "spatie/laravel-markdown": "^2.5.2",
        "spatie/laravel-medialibrary": "^11.7.1",
        "spatie/laravel-model-states": "^2.7.1",
        "spatie/laravel-permission": "^6.7",
        "spatie/laravel-prefixed-ids": "^1.4.0",
        "spatie/laravel-site-search": "^2.3",
        "spatie/laravel-sitemap": "^7.2.1",
        "spatie/laravel-sluggable": "^3.6.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0.0",
        "fakerphp/faker": "^1.23.1",
        "laravel/envoy": "^2.9",
        "laravel/pint": "^1.16.1",
        "laravel/sail": "^1.29.3",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.1.1",
        "pestphp/pest": "^2.34.8",
        "pestphp/pest-plugin-laravel": "^2.4",
        "phpunit/phpunit": "^10.5.17"
    },
    "autoload": {
        "files": [
            "src/Foundation/Helpers.php"
        ],
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domain/",
            "Foundation\\": "src/Foundation/",
            "Support\\": "src/Support/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
